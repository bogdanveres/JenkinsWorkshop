node ('built-in'){
    stage('clone repository') {               
        checkout scm    
    } 

    stage('show env var') {
        sh "echo ${env.WORKSPACE}"
    }

    stage('run script') {         
        sh 'chmod +x HelloWorld.sh'
        sh 'ls'
        sh './HelloWorld.sh'   
    }
    
    stage('Clean up Workspace')
    {
        cleanWs deleteDirs: true
    }
}
